{
	"env": {
		"commonjs": true,
		"es6": true,
		"node": true,
		"mocha": true,
		"amd": true,
		"jest": true
	},
	"extends": [
		"airbnb-base"
	],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"rules": {
		"max-len": [
			"error",
			{
				"code": 200
			}
		],
		"no-console": "warn",
		"indent": [
			"error",
			2
		],
		"global-require": "off",
		"import/no-dynamic-require": "off",
		"func-names": "off",
		"no-restricted-syntax": "off",
		"no-await-in-loop": "off",
		"no-param-reassign": "off",
		"radix": "off"
	}
}