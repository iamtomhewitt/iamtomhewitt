{
	"env": {
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"extends": [
		"airbnb-base"
	],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"rules": {
		"no-console": "off",
		"indent": [
			"error",
			2
		],
		"no-restricted-syntax": "off",
		"no-await-in-loop": "off",
		"no-param-reassign": "off",
		"no-return-assign": "off",
		"comma-dangle": ["error", "never"]
	}
}